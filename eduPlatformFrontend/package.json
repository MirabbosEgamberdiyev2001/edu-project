{
  "name": "edu-platform-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm -r --parallel run dev",
    "dev:admin": "pnpm --filter @eduplatform/admin dev",
    "dev:main": "pnpm --filter @eduplatform/main dev",
    "build": "pnpm -r run build",
    "build:admin": "pnpm --filter @eduplatform/admin build",
    "build:main": "pnpm --filter @eduplatform/main build",
    "preview:admin": "pnpm --filter @eduplatform/admin preview",
    "preview:main": "pnpm --filter @eduplatform/main preview",
    "type-check": "pnpm -r run type-check",
    "clean": "pnpm -r exec rm -rf dist node_modules"
  },
  "devDependencies": {
    "typescript": "~5.6.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
